-	do {							\
-		uint32_t tmp_ = RREG32_SMC(reg);		\
-		tmp_ &= (mask);					\
-		tmp_ |= ((val) & ~(mask));			\
-		WREG32_SMC(reg, tmp_);				\
-	} while (0)
