> @@ -23,7 +23,7 @@ enum NvmeCapShift {
>      CAP_AMS_SHIFT      =3D 17,
>      CAP_TO_SHIFT       =3D 24,
>      CAP_DSTRD_SHIFT    =3D 32,
> -    CAP_NSSRS_SHIFT    =3D 33,
> +    CAP_NSSRS_SHIFT    =3D 36,
>      CAP_CSS_SHIFT      =3D 37,
>      CAP_MPSMIN_SHIFT   =3D 48,
>      CAP_MPSMAX_SHIFT   =3D 52,
>=20

